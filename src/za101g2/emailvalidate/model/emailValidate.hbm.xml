<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="za101g2.emailvalidate.model.EmailValidateVO" table="emailvalidate" >
        
        <id name="memId" column="memId" />
             
        <property name="email" column="email" unique="true"/>
        <property name="validateCode" column="validateCode" unique="true"/>
       
    </class>
</hibernate-mapping>

