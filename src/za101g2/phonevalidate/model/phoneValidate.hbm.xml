<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="za101g2.phonevalidate.model.PhoneValidateVO" table="phonevalidate" >
        
        <id name="memId" column="memId" />
             
        <property name="phone" column="phone" unique="true"/>
        <property name="validateCode" column="validateCode"/>
        <property name="errorCount" column="errorCount" insert="false"/>
       
    </class>
</hibernate-mapping>

